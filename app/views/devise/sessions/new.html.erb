<div class="form">
  <ul class="tab-group">
    <!-- # <li class="tab active"><a href= "#">Sign Up</a></li> -->
    <li class="tab active">
    <%= link_to( "Log in", new_user_session_path, :method => :get)%></li>
    <li class="tab">
    <%= link_to("Sign up", new_user_registration_path, :method => :get )%></li>
    <!-- <%#= link_to("Logout", destroy_user_session_path, :method => :delete) %></li> -->
    <!-- <li class="tab"><a href="#">Log In</a></li> -->
    <!-- <li class="tab"><a href="/signup">Sign Up</a></li> -->
        <!-- <li class="tab"><a href="#login">Log In</a></li> -->
  </ul>
  
  <div class="tab-content">
    <!-- <div id="login">    -->
      <h1>Welcome Back!</h1>
      <%= form_for(resource, as: resource_name, html: { autocomplete: "off" }, url: session_path(resource_name)) do |f| %>
        <div class="field-wrap">
            <label>
              Email Address<span class="req">*</span>
            </label>
            <%= f.email_field :email, :required => true, :autocomplete => false, autofocus: true %>
          </div>
        <div class="field-wrap">
          <label>
            Password<span class="req">*</span>
          </label>
          <%= f.password_field :password, :required => true, autocomplete: "off"%>
        </div>
        <%= f.submit "Log in", :class => "button button-block"  %>
      <% end %>
      <%#= render "devise/shared/links" %>
    </div>
  </div>
</div>


        
          
          